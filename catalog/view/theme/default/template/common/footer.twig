<footer>

<div class="container">
  <div class="row newsbrd">
      {{ ftop_full }}
    </div>
</div>
    <div class="middle-footer">
      <div class="container">
        <div class="row mft">
      
    {{ footer_left }}

    {% if footer_left and footer_right %}
    {% set class = 'col-sm-8' %}
    {% elseif footer_left or footer_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
<div class="col-sm-9 col-md-9 col-xs-12">
    <div class="row">
      {% if informations %}
      <div class="col-sm-4 col-xs-12 fborder">
        <h5>{{ text_information }}
          <button type="button" class="btn toggle collapsed" data-toggle="collapse" data-target="#info"></button>
        </h5>
        <div id="info" class="collapse footer-collapse">
        <ul class="list-unstyled">
         {% for information in informations %}
          <li><a href="{{ information.href }}">{{ information.title }}</a></li>
          {% endfor %}
           <li><a href="{{ manufacturer }}">{{ text_manufacturer }}</a></li>
        </ul>
        </div>
      </div>
      {% endif %}
      <div class="col-sm-4 col-xs-12 fborder">
        <h5>{{ text_account }}
          <button type="button" class="btn toggle collapsed" data-toggle="collapse" data-target="#account"></button>
        </h5>
        <div id="account" class="collapse footer-collapse">
        <ul class="list-unstyled lastb">
          <li><a href="{{ account }}">{{ text_account }}</a></li>
          <li><a href="{{ order }}">{{ text_order }}</a></li>
          <li><a href="{{ wishlist }}">{{ text_wishlist }}</a></li>
          <li><a href="{{ newsletter }}">{{ text_newsletter }}</a></li>
          <li><a href="{{ special }}">{{ text_special }}</a></li>
        </ul>
        </div>
      </div>
      <div class="col-sm-4 col-xs-12 fborder">
        <h5>{{ text_service }}
          <button type="button" class="btn  toggle collapsed" data-toggle="collapse" data-target="#service"></button>
        </h5>
        <div id="service" class="collapse footer-collapse">
        <ul class="list-unstyled lastb">
          <li><a href="{{ contact }}">{{ text_contact }}</a></li>
          <li><a href="{{ return }}">{{ text_return }}</a></li>
          <li><a href="{{ sitemap }}">{{ text_sitemap }}</a></li>
          <li><a href="{{ voucher }}">{{ text_voucher }}</a></li>
          <li><a href="{{ affiliate }}">{{ text_affiliate }}</a></li>
        </ul>
        </div>
      </div>
    </div>
      </div>
</div></div>
    </div>

  <div class="container">
    <div class="row copy">
      <div class="col-md-5 col-sm-5 text-left">{{ powered }}</div>
      <div class="col-md-7 col-sm-7"> <div class="row spay">{{ fbottom_full }}</div></div>
    </div>
  </div>

</footer>
<a href="" id="scroll" title="Scroll to Top" style="display: block;">
    <svg width="32px" height="32px"><use xlink:href="#scroll1"></use></svg>
</a>
     


